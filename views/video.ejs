<html>
  <head>
    <title><%= title %></title>
    <!-- CSS only -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <!-- JS, Popper.js, and jQuery -->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
  
    <!--Materials Icon-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    <!--custom-->
    <link rel="stylesheet" href="/stylesheets/video.css">
  </head>
  <body>
    <div class="header">
      <div class="title">iSeeYa</div>
      <div class="material-icons view-display-btn">person</div>
    </div>
    <div class="videos-cont">
      <div class="video-cont">
        <video class="video" id="your-video" muted></video>
      </div>

      <!--delete this after-->
      <div class="video-cont">
        <video class="video" id="other-video" muted></video>
      </div>

    </div>
    <div class="footer">
      <div class="circle-btns-cont">
        <div class="material-icons circle-btn" id="mic-call-btn">mic</div>
        <div class="material-icons circle-btn" id="end-call-btn">phone</div>
        <div class="material-icons circle-btn" id="video-call-btn">videocam</div>
      </div>
    </div>

    <script src="bundle.js"></script>
    <script src="/socket.io/socket.io.js"></script>
  </body>
</html>
